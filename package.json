{
  "name": "wine-scanner",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export -p web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "deploy:web": "expo export -p web && npx expo publish --platform web",
    "deploy:vercel": "expo export -p web && vercel --prod --yes",
    "setup:db": "node scripts/run-schema-migration.js",
    "setup:db:cli": "./scripts/setup-supabase.sh",
    "setup:db:direct": "node scripts/run-schema-db.js",
    "setup:db:open": "node scripts/open-sql-editor.js"
  },
  "dependencies": {
    "@expo-google-fonts/crimson-pro": "^0.4.2",
    "@expo-google-fonts/playfair-display": "^0.4.2",
    "@react-navigation/native": "^7.1.20",
    "@react-navigation/native-stack": "^7.6.3",
    "@supabase/supabase-js": "^2.81.1",
    "@types/react": "~19.1.10",
    "expo": "~54.0.23",
    "expo-camera": "^17.0.9",
    "expo-font": "^14.0.9",
    "expo-image-picker": "^17.0.8",
    "expo-status-bar": "~3.0.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.18.0",
    "react-native-web": "^0.21.0",
    "typescript": "~5.9.2"
  },
  "private": true,
  "devDependencies": {
    "pg": "^8.16.3"
  }
}
